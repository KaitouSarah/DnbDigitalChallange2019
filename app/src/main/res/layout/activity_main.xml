<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <Button
            android:text="Budgivning"
            android:layout_width="188dp"
            android:layout_height="113dp"
            android:padding="18dp"
            android:drawableBottom="@drawable/biddingicon"
            android:id="@+id/budButton" android:background="@color/sjogronnAlt"
            android:textColor="@android:color/white"
            android:onClick="button_bidding"
            tools:ignore="MissingConstraints" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/verdivurderingButton"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Visninger"
            android:layout_width="188dp"
            android:layout_height="113dp"
            android:padding="18dp"
            android:drawableBottom="@drawable/houseicon"
            android:id="@+id/visningerButton"
            android:background="@color/sjogronnAlt" android:textColor="@android:color/white"
            tools:ignore="MissingConstraints" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/finansieringButton" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
    />
    <Button
            android:text="Finansieringsbevis"
            android:layout_width="188dp"
            android:layout_height="113dp"
            android:padding="18dp"
            android:drawableBottom="@drawable/adddocumenticon"
            android:id="@+id/finansieringButton"
            android:background="@color/sjogronnAlt" android:textColor="@android:color/white"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/tipButton" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
    />
    <Button
            android:text="Tips og vilkÃ¥r"
            android:layout_width="188dp"
            android:layout_height="113dp"
            android:padding="18dp"
            android:drawableBottom="@drawable/infoicon"
            android:id="@+id/tipButton" android:background="@color/sjogronnAlt"
            android:textColor="@android:color/white"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
    />
    <Button
            android:text="Verdivurdering"
            android:layout_width="188dp"
            android:layout_height="113dp"
            android:padding="18dp"
            android:drawableBottom="@drawable/valueicon"
            android:id="@+id/verdivurderingButton"
            android:background="@color/sjogronnAlt" android:textColor="@android:color/white"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/feebbackButton"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Gi tilbakemelding"
            android:layout_width="188dp"
            android:layout_height="113dp"
            android:padding="18dp"
            android:drawableBottom="@drawable/feedbackicon"
            android:id="@+id/feebbackButton" android:background="@color/sjogronnAlt"
            android:textColor="@android:color/white"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/backgroundGirl"
            android:background="@drawable/backgroundgirl"
            tools:ignore="MissingConstraints"/>
    <ImageView
            android:layout_width="226dp"
            android:layout_height="45dp" app:srcCompat="@drawable/dnbeiendeomlogo"
            android:id="@+id/dnbEiendomLogo"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="6dp"/>

</android.support.constraint.ConstraintLayout>